<div class="mb-3">
  <label for="{{fieldName}}" class="form-label"> 
    {{fieldName.capitalizeAscii()}}
    {% if isUpdateForm and value.isSome() %}
      <span class="text-muted">
        - Stored in: "{{value.get()}}"
      </span> 
    {% endif %}
  </label>
  <input 
  type="file" 
  class="form-control" 
  id="{{fieldName}}" 
  form="form"
  name="{{fieldName}}">
 
</div>
